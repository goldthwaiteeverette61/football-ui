# .env.production

# 页面标题
VITE_APP_TITLE = 红色逻辑-管理后台

# 生产环境配置
VITE_APP_ENV = 'production'

# ▼▼▼ [核心修改] 直接指定完整的生产环境API地址 ▼▼▼
VITE_APP_BASE_API = 'https://api.sportlt.dpdns.org'

# 部署应用的子路径
VITE_APP_CONTEXT_PATH = '/'

# [提示] 如果您使用监控和任务调度功能，请将下面的地址替换为您的真实地址
VITE_APP_MONITOR_ADMIN = 'http://your-production-domain.com:9090/admin/applications'
VITE_APP_SNAILJOB_ADMIN = 'http://your-production-domain.com:8800/snail-job'

# 接口加密功能开关(如需关闭 后端也必须对应关闭)
VITE_APP_ENCRYPT = true
# 接口加密传输 RSA 公钥与后端解密私钥对应 如更换需前后端一同更换
VITE_APP_RSA_PUBLIC_KEY = ''
# 接口响应解密 RSA 私钥与后端加密公钥对应 如更换需前后端一同更换
VITE_APP_RSA_PRIVATE_KEY = ''

# 客户端id
VITE_APP_CLIENT_ID = 'e5cd7e4891bf95d1d19206ce24a7b32e'

# websocket 开关 默认使用sse推送
VITE_APP_WEBSOCKET = false

# sse 开关
VITE_APP_SSE = false